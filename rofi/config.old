rofi.location:          0
rofi.color-enabled:     true
!                       'background', 'border'
rofi.color-window:      argb:dc111111, argb:dc111111
! State:                'bg',          'fg',   'bgalt',       'hlbg',        'hlfg'
rofi.color-normal:      argb:00333333, #ffffff, argb:00333333, argb:00333333, argb:ff00BCD4
rofi.color-urgent:      argb:00333333, #ffffff, argb:00333333, argb:00333333, argb:ff00BCD4
rofi.color-active:      argb:00333333, #ffffff, argb:00333333, argb:00333333, argb:ff00BCD4
rofi.separator-style:   none
rofi.fullscreen:        true
rofi.padding:           475
rofi.hide-scrollbar:    true
rofi.line-margin:       5
rofi.fake-transparency: false
rofi.yoffset:           10
rofi.xoffset:           50
rofi.width:             1000
rofi.opacity:           50
rofi.font:              Noto 24


*cursorColor:                         #FFFF00
*colorIT:                             #E3AB8D
*colorBD:                             #F0E5DF
*colorUL:                             #E91E63
*VT100.colorBDMode:                   true
*VT100.colorBD:                       #F44336
*VT100.colorULMode:                   true
*VT100.colorUL:                       #F06292
*VT100.veryBoldColors:                10


